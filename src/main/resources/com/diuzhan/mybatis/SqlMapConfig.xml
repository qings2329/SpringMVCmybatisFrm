<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE configuration 
PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN" 
"http://ibatis.apache.org/dtd/ibatis-3-config.dtd"> 
<configuration> 
    <typeAliases> 
<!--         Entities 参数实体 -->
<!--         com.diuzhan.web.bean 包中的 所有Bean, 取个别名 -->
<!--          比如TestTable类就用Test来表示 -->
        <typeAlias type="com.diuzhan.web.bean.TestTable" 
        alias="Test" /> 
    </typeAliases> 
    <mappers> 
<!--         对应Bean类的xml配置文件的路径信息 -->
        <mapper resource="com/diuzhan/mybatis/mapper/Test.xml" /> 
    </mappers> 
</configuration>